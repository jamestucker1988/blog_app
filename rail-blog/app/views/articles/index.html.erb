<h1>MY Blog App</h1>

<table class="table">
  <thead>
    <tr>
      <th scope="col">Title</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td class="col">
      <% @articles.each do |article| %>
      <li>
      <br/>
      <%= link_to article.title, article, class:"btn btn-outline-dark" %>
      </li>
      <% end %>
     
      </td>
    </tr>
  </tbody>
</table>

